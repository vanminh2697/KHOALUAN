{
    "manifest_version":2,
    "name": "Review Laptop",
    "version":"1.0",
    "browser_action":{
     "default_popup":"popup.html",
     "default_icon": "ga.png"
    },
    "content_scripts":[
     {
      "matches":["http://*/*","https://*/*"],
      "js":["run.js"]
     }
    ],
    "permissions" : [
      "http://localhost:5000/"
    ],
    "background": {
      "scripts": ["event.js"]
    },
    "externally_connectable": {
      "matches": ["http://localhost:5000/"]
    },
    "content_security_policy":"script-src 'self' https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js; object-src 'self'"
   }
