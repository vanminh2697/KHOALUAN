{
    "manifest_version":2,
    "name": "Ungdung",
    "version":"1.0",
    "browser_action":{
     "default_popup":"popup.html"
    },
    "content_scripts":[
     {
      "matches":["http://*/*","https://*/*"],
      "js":["run.js"]
     }
    ],
    "permissions" : [
      "http://localhost:5000/"
    ],
    "background": {
      "scripts": ["event.js"]
    },
    "externally_connectable": {
      "matches": ["http://localhost:5000/"]
    }
   }